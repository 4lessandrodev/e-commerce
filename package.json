{
	"name": "api",
	"version": "0.0.1",
	"description": "E-commerce to sell organics products",
	"author": "Alessandro",
	"private": true,
	"license": "MIT",
	"engines": {
		"node": ">14.15.0"
	},
	"scripts": {
		"prebuild": "rimraf dist && yarn",
		"install:all": "yarn",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start:server": "nest start",
		"start:dev": "env-cmd -f ./.env nest start --watch",
		"start:prod": "env-cmd -f ./.env.production nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
		"test:verbose": "jest --passWithNoTests --noStackTrace --runInBand",
		"test:staged": "jest --silent --passWithNoTests --runInBand",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"docs": "./node_modules/typedoc/bin/typedoc --out ./docs ./src",
		"prepare": "husky install"
	},
	"dependencies": {
		"@nestjs/common": "<7.6.18",
		"@nestjs/core": "<7.6.18",
		"@nestjs/jwt": "<7.2.0",
		"@nestjs/mongoose": "<7.2.4",
		"@nestjs/passport": "<7.1.6",
		"@nestjs/platform-express": "<7.6.18",
		"@types/passport-jwt": "^3.0.5",
		"bcrypt": "^5.0.1",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.1",
		"currency.js": "^2.0.4",
		"date-fns": "^2.21.2",
		"env-var": "^7.0.0",
		"module-alias": "^2.2.2",
		"mongoose": "^5.12.7",
		"mongoose-autopopulate": "^0.13.0",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "6.6.7",
		"types-ddd": "^0.0.9",
		"ua-parser-js": "^0.7.28",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@microsoft/tsdoc": "^0.13.2",
		"@nestjs/cli": "<7.6.0",
		"@nestjs/schematics": "<7.3.1",
		"@nestjs/testing": "<7.6.18",
		"@types/bcrypt": "^3.0.1",
		"@types/date-fns": "^2.6.0",
		"@types/delay": "^3.1.0",
		"@types/express": "^4.17.8",
		"@types/faker": "^5.5.4",
		"@types/jest": "^26.0.23",
		"@types/multer": "^1.4.5",
		"@types/node": "<15.6.1",
		"@types/supertest": "^2.0.11",
		"@types/ua-parser-js": "^0.7.35",
		"@types/uuid": "^8.3.0",
		"@types/validator": "^13.1.0",
		"@typescript-eslint/eslint-plugin": "^4.22.1",
		"@typescript-eslint/parser": "^4.22.1",
		"delay": "^5.0.0",
		"env-cmd": "^10.1.0",
		"eslint": "^7.25.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"faker": "^5.5.3",
		"git-commit-msg-linter": "^3.1.0",
		"husky": "^6.0.0",
		"jest": "^26.6.3",
		"lint-staged": "^11.0.0",
		"prettier": "^2.1.2",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.6",
		"ts-loader": "^9.1.2",
		"ts-node": "^9.0.0",
		"tsconfig-paths": "^3.9.0",
		"typedoc": "^0.20.36",
		"typescript": "^4.0.5",
		"update-check": "^1.5.4"
	},
	"_moduleAliases": {
		"@domain/*": "dist/Domain/*",
		"@shared/*": "dist/Shared/*",
		"@infra/*": "dist/Infra/*",
		"@repo/*": "dist/Repo/*",
		"@app/*": "dist/Application/*",
		"@": "dist"
	}
}